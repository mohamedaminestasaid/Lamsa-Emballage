{
  "version": 2,
  "name": "Lamsa",
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
         "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}
